<% layout('/layouts/boilerplate') %>
<body>
    <h1>Edit your list</h1>
    <form method="post" action="/listing/<%= data.id %>/edit/?_method=patch">
        <lable for="">Title<input name="title" type="text" value="<%= data.title %>" id=""></lable>
        <br>
        <lable for="">description<input name="description" type="text" value="<%= data.description %>" id=""></lable>
        <br>
        <lable for="">image link<input name="image" type="text" value="<%= data.image %>" id=""></lable>
        <br>
        <lable for="">price<input name="price" type="text" value="<%= data.price %>" id=""></lable>
        <br>
        <lable for="">location<input name="location" type="text" value="<%= data.location %>" id=""></lable>
        <br>
        <lable for="">country<input name="country" type="text" value="<%= data.country %>" id=""></lable>
        <br>
    <button>make changes</button>
    </form>
</body>
